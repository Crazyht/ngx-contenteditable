{
  "name": "ngx-contenteditable",
  "version": "0.9.1",
  "description": "Library QuickStart package.json from the documentation, supplemented with testing support",
  "main": "./bundles/ngx-contenteditable.umd.js",
  "module": "./ngx-contenteditable.es5.js",
  "es2015": "./ngx-contenteditable.js",
  "typings": "./ngx-contenteditable.d.ts",
  "author": "",
  "license": "MIT",
  "repository": {},
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean": "rimraf out-tsc dist/* \"src/lib/**/*.js\" \"src/lib/**/*.map\"",
    "prebuild": "npm run clean",
    "build": "node build.js",
    "build-demo": "tsc -p src/demo/",
    "build-demo:watch": "tsc -p src/demo/ -w",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build-demo",
    "start": "concurrently \"npm run build-demo:watch\" \"npm run serve\"",
    "build-test": "tsc -p src/lib/tsconfig.spec.json",
    "build-test:watch": "tsc -p src/lib/tsconfig.spec.json -w",
    "pretest": "npm run build-test",
    "test": "concurrently \"npm run build-test:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build-test",
    "test:once": "karma start karma.conf.js --single-run",
    "preintegration": "npm run build && cd integration && npm run clean && npm install",
    "integration": "npm run integration:aot && npm run integration:jit",
    "integration:jit": "cd integration && npm run e2e",
    "integration:aot": "cd integration && npm run e2e:aot",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "changelog": "standard-version",
    "release": "npm run build && npm run test:once && npm run integration && npm publish dist"
  },
  "peerDependencies": {
    "@angular/core": ">=4.0.0 <5.0.0 || >=4.0.0-beta <5.0.0",
    "@angular/forms": ">=4.0.0 <5.0.0 || >=4.0.0-beta <5.0.0"
  },
  "devDependencies": {
    "@angular/common": "~4.4.5",
    "@angular/compiler": "~4.3.1",
    "@angular/compiler-cli": "~4.3.1",
    "@angular/core": "~4.3.1",
    "@angular/forms": "~4.3.1",
    "@angular/platform-browser": "~4.3.1",
    "@angular/platform-browser-dynamic": "~4.3.1",
    "@types/execa": "^0.7.0",
    "@types/jasmine": "2.5.53",
    "@types/karma-coverage": "^0.5.32",
    "@types/node": "^8.0.16",
    "camelcase": "^4.1.0",
    "concurrently": "3.5.0",
    "core-js": "^2.4.1",
    "execa": "^0.7.0",
    "font-awesome": "~4.7.0",
    "glob": "^7.1.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-coverage": "^0.1.4",
    "lite-server": "^2.2.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.4.2",
    "standard-version": "^4.2.0",
    "systemjs": "^0.20.14",
    "tslint": "^5.4.3",
    "typescript": "^2.4.1",
    "zone.js": "^0.8.12"
  },
  "dependencies": {
    "ngx-bootstrap": "^1.7.1",
    "moment": "^2.18.1"
  }
}
